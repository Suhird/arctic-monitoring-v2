    "def export_sample_for_testing(sample_idx=0):\n",
    "    \"\"\"Export a sample for testing the trained model\"\"\"\n",
    "    sample = manifest['samples'][sample_idx]\n",
    "    \n",
    "    # Load image\n",
    "    img_path = f\"../data/labeled_ice_imagery/images/{sample['image']}\"\n",
    "    img = Image.open(img_path)\n",
    "    \n",
    "    # Save to test directory\n",
    "    test_dir = '../test_samples'\n",
    "    os.makedirs(test_dir, exist_ok=True)\n",
    "    \n",
    "    output_path = f\"{test_dir}/test_sample_{sample_idx}.png\"\n",
    "    img.save(output_path)\n",
    "    \n",
    "    print(f\"✅ Exported sample to: {output_path}\")\n",
    "    print(f\"\\nUse this sample to test your trained model!\")\n",
    "    \n",
    "    # Display the sample\n",
    "    plt.figure(figsize=(8, 8))\n",
    "    plt.imshow(img)\n",
    "    plt.title(f'Test Sample {sample_idx}', fontsize=14, fontweight='bold')\n",
    "    plt.axis('off')\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "export_sample_for_testing(0)"
   ]
