    "def print_dataset_summary():\n",
    "    \"\"\"Print comprehensive dataset summary\"\"\"\n",
    "    print(\"=\"*60)\n",
    "    print(\"ARCTIC ICE DATASET SUMMARY\")\n",
    "    print(\"=\"*60)\n",
    "    print(f\"\\nDataset Name: {manifest['dataset_name']}\")\n",
    "    print(f\"Created: {manifest['created']}\")\n",
    "    print(f\"Total Samples: {manifest['num_samples']}\")\n",
    "    print(f\"Image Format: {manifest['image_format']}\")\n",
    "    print(f\"Label Format: {manifest['label_format']}\")\n",
    "    \n",
    "    print(f\"\\nClasses ({len(manifest['classes'])}):")\n",
    "    for cls in manifest['classes']:\n",
    "        print(f\"  [{cls['id']}] {cls['name']}: {cls['description']}\")\n",
    "    \n",
    "    # Calculate dataset size\n",
    "    total_size = 0\n",
    "    for sample in manifest['samples']:\n",
    "        img_path = f\"../data/labeled_ice_imagery/images/{sample['image']}\"\n",
    "        if os.path.exists(img_path):\n",
    "            total_size += os.path.getsize(img_path)\n",
    "    \n",
    "    print(f\"\\nDataset Size: {total_size / (1024*1024):.2f} MB\")\n",
    "    print(f\"Average per sample: {total_size / manifest['num_samples'] / 1024:.2f} KB\")\n",
    "    \n",
    "    print(\"\\n\" + \"=\"*60)\n",
    "    print(\"✅ Dataset is ready for preprocessing!\")\n",
    "    print(\"\\nNext steps:\")\n",
    "    print(\"  1. Run: cd ../data && python preprocessing.py\")\n",
    "    print(\"  2. Run: cd ../training && python train_ice_classifier.py\")\n",
    "    print(\"=\"*60)\n",
    "\n",
    "print_dataset_summary()"
   ]
  },
